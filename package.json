    "start-frontend": "nx serve frontend",
    "start": "nx serve cli",
    "build": "nx build cli",
    "build-ext": "tsc --project apps/extension/tsconfig.app.json && cp apps/extension/manifest.json dist/apps/extension",
    "precommit-build": "yarn build && yarn build-ext && git add dist/",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "validate": "npm ls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "np && yarn changelog && git commit CHANGELOG.md -m 'chore: update changelog'"
  },
  "files": [
    "main.js",
    "dist/apps/cli/main.js"
