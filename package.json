  "scripts": {
    "ng": "nx",
    "postinstall": "if [ -e ./decorate-angular-cli.js ]; then node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main; fi",
    "start-api": "nx serve api",
    "start-frontend": "nx serve frontend --port 4203",
    "start": "nx serve cli",
    "build": "nx build cli",
    "build-ext": "tsc --project apps/extension/tsconfig.app.json && cp apps/extension/manifest.json dist/apps/extension",
    "build-frontend": "nx build frontend",
    "build-api": "nx build api",
    "precommit-build": "rm -rf ./node_modules/.cache/nx && yarn build && yarn build-ext && yarn build-frontend && yarn build-api && git add dist/",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "validate": "npm ls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "np && yarn changelog && git commit CHANGELOG.md -m 'chore: update changelog'"
  },
  "files": [
    "main.js",
    "main-server.js",
    "dist/apps/cli/main.js",
    "dist/apps/api/main.js",
    "dist/apps/frontend"
  ],
  "pre-commit": [
    "precommit-build"
  ],
